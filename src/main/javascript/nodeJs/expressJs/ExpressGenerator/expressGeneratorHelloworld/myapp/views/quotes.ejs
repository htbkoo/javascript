<!DOCTYPE html>
<html lang="en">
<% include template/header.ejs %>
<head>
    <!--suppress HtmlUnknownTarget -->
    <link rel="stylesheet" href="/stylesheets/index.css"/>
    <link rel="stylesheet" href="/css/bootstrap.min.css">
</head>
<body>
<div class="container-fluid">
    <div class="page-header">
        <h1>Quotes page
            <small>Get quotes for underlyings</small>
        </h1>
    </div>
    <div class="row">
        <div class="col-md-1"></div>
        <div class="col-md-10">
            <% if (typeof underlyings !== "undefined"){ %>
            <ul class="list-group">
                <% underlyings.getKeysAsArray().forEach(function(k){ %>
                <li class="list-group-item"><%= k; %></li>
                <% }); %>
            </ul>
            <% } %>
        </div>
        <div class="col-md-1"></div>
    </div>
</div>

<% include template/scripts.ejs %>
<script data-main="/js/quotes.js" src="/js/lib/require.js"></script>
</body>
</html>